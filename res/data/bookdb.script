CREATE MEMORY TABLE PUBLIC.LENT_BOOK(BISBN CHARACTER(17),CID INTEGER,LDATE DATE NOT NULL,LTIME TIME NOT NULL,LNUMBER INTEGER NOT NULL,LPERIOD SMALLINT NOT NULL,PRIMARY KEY(BISBN,CID,LDATE),CHECK(PUBLIC.LENT_BOOK.LNUMBER>0),CHECK(PUBLIC.LENT_BOOK.LPERIOD>0),FOREIGN KEY(BISBN) REFERENCES PUBLIC.BOOK_INFO(BISBN),FOREIGN KEY(CID) REFERENCES PUBLIC.CUSTOMER_INFO(CID))

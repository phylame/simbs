CREATE MEMORY TABLE PUBLIC.BOOK(BISBN CHARACTER(17) PRIMARY KEY,BNAME VARCHAR(128) NOT NULL,BVERSION VARCHAR(64),BAUTHORS VARCHAR(128) NOT NULL,BCOVER VARCHAR(512),BDATE DATE NOT NULL,BCATEGORY VARCHAR(128),BPUBLISHER VARCHAR(128) NOT NULL,BPURCHASE DECIMAL(10,2) DEFAULT 0.00 NOT NULL,BPRICE DECIMAL(10,2) NOT NULL,BINTRO VARCHAR(512),CHECK(PUBLIC.BOOK.BPURCHASE>=0),CHECK(PUBLIC.BOOK.BPRICE>=0.0))
CREATE MEMORY TABLE PUBLIC.CUSTOMER(CID INTEGER PRIMARY KEY,CNAME VARCHAR(64) NOT NULL,CPHONE VARCHAR(16),CEMAIL VARCHAR(48),CDATE DATE DEFAULT LOCALTIMESTAMP NOT NULL,CLEVEL SMALLINT DEFAULT 0 NOT NULL,CLIMIT SMALLINT DEFAULT 10 NOT NULL,CCOMMENT VARCHAR(512),CHECK(PUBLIC.CUSTOMER.CID>=0),CHECK(PUBLIC.CUSTOMER.CLEVEL>=0),CHECK(PUBLIC.CUSTOMER.CLIMIT>=0))
CREATE MEMORY TABLE PUBLIC.INVENTORY(BISBN CHARACTER(17) PRIMARY KEY,INUMBER INTEGER NOT NULL,CHECK(PUBLIC.INVENTORY.INUMBER>=0),FOREIGN KEY(BISBN) REFERENCES PUBLIC.BOOK(BISBN))
CREATE MEMORY TABLE PUBLIC.SALE(SID INTEGER PRIMARY KEY,BISBN CHARACTER(17) NOT NULL,CID INTEGER NOT NULL,SDATE DATE NOT NULL,STIME TIME NOT NULL,SNUMBER INTEGER NOT NULL,STOTAL DECIMAL(10,2) NOT NULL,SCOMMENT VARCHAR(128),CHECK(PUBLIC.SALE.SID>0),CHECK(PUBLIC.SALE.SNUMBER>0),CHECK(PUBLIC.SALE.STOTAL>=0),FOREIGN KEY(BISBN) REFERENCES PUBLIC.BOOK(BISBN),FOREIGN KEY(CID) REFERENCES PUBLIC.CUSTOMER(CID))
CREATE MEMORY TABLE PUBLIC.RENTAL(RID INTEGER PRIMARY KEY,BISBN CHARACTER(17) NOT NULL,CID INTEGER NOT NULL,RDATE DATE NOT NULL,RTIME TIME NOT NULL,RNUMBER INTEGER NOT NULL,RPERIOD SMALLINT NOT NULL,RPRICE DECIMAL(10,2) DEFAULT 0.00 NOT NULL,RDEPOSIT DECIMAL(10,2) NOT NULL,RREVENUE DECIMAL(10,2) NOT NULL,RCOMMENT VARCHAR(128),CHECK(PUBLIC.RENTAL.RID>0),CHECK(PUBLIC.RENTAL.RNUMBER>=0),CHECK(PUBLIC.RENTAL.RPERIOD>0),CHECK(PUBLIC.RENTAL.RPRICE>=0),CHECK(PUBLIC.RENTAL.RDEPOSIT>=0),CHECK(PUBLIC.RENTAL.RREVENUE>=0),FOREIGN KEY(BISBN) REFERENCES PUBLIC.BOOK(BISBN),FOREIGN KEY(CID) REFERENCES PUBLIC.CUSTOMER(CID))
CREATE MEMORY TABLE PUBLIC.BILL(LNO INTEGER PRIMARY KEY,LDATE DATE NOT NULL,LTIME TIME NOT NULL,LEVENT INTEGER NOT NULL,LID INTEGER NOT NULL,CHECK(PUBLIC.BILL.LNO>0),CHECK((PUBLIC.BILL.LEVENT) IN ((1),(2),(3),(4))),CHECK(PUBLIC.BILL.LID>0))
CREATE MEMORY TABLE PUBLIC.PROMOTION(PID INTEGER PRIMARY KEY,POBJECT SMALLINT NOT NULL,PVALUE DECIMAL(10,2) DEFAULT 10.00 NOT NULL,PSTART DATE NOT NULL,PEND DATE NOT NULL,PCOMMENT VARCHAR(512),CHECK(PUBLIC.PROMOTION.PID>0),CHECK((PUBLIC.PROMOTION.POBJECT) IN ((1),(2))),CHECK(PUBLIC.PROMOTION.PVALUE>0),CHECK(PUBLIC.PROMOTION.PEND>PUBLIC.PROMOTION.PSTART))
CREATE MEMORY TABLE PUBLIC.STOCK(TID INTEGER PRIMARY KEY,BISBN CHARACTER(17),TDATE DATE NOT NULL,TTIME TIME NOT NULL,TNUMBER INTEGER NOT NULL,TPURCHASE DECIMAL(10,2) NOT NULL,TTOTAL DECIMAL(10,2) NOT NULL,TCOMMENT VARCHAR(512),CHECK(PUBLIC.STOCK.TID>0),CHECK(PUBLIC.STOCK.TNUMBER>0),CHECK(PUBLIC.STOCK.TPURCHASE>=0),CHECK(PUBLIC.STOCK.TTOTAL>=0),FOREIGN KEY(BISBN) REFERENCES PUBLIC.BOOK(BISBN))
